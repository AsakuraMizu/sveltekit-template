    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.39.0",
    "globals": "^15.3.0",
    "prettier": "^3.3.1",
    "prettier-plugin-svelte": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "svelte": "5.0.0-next.149",
    "svelte-check": "^3.8.0",
    "tailwindcss": "4.0.0-alpha.15",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
