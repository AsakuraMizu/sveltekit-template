{
  "name": "sveltekit-template",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build:site": "vite build",
    "build:lib": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pret": "prettier --plugin-search-dir=. --check .",
    "pret:fix": "prettier --plugin-search-dir=. --write ."
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.16",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.15.5",
    "@sveltejs/package": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@unocss/core": "^0.51.4",
    "@unocss/eslint-config": "^0.51.4",
    "@unocss/extractor-svelte": "^0.51.4",
    "@unocss/preset-attributify": "^0.51.4",
    "@unocss/preset-icons": "^0.51.4",
    "@unocss/preset-uno": "^0.51.4",
    "@unocss/reset": "^0.51.4",
    "@unocss/transformer-directives": "^0.51.4",
    "@unocss/transformer-variant-group": "^0.51.4",
    "@unocss/vite": "^0.51.4",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.26.0",
    "jsdom": "^21.1.1",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-eslint-parser": "^0.26.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  }
}
