{
  "name": "sveltekit-template",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build:site": "vite build",
    "build:lib": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write ./**/*.{js,cjs,ts,html,css,svelte,md,json}"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.21",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.24.1",
    "@sveltejs/package": "^2.2.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@unocss/core": "^0.55.7",
    "@unocss/eslint-config": "^0.55.7",
    "@unocss/extractor-svelte": "^0.55.7",
    "@unocss/preset-attributify": "^0.55.7",
    "@unocss/preset-icons": "^0.55.7",
    "@unocss/preset-uno": "^0.55.7",
    "@unocss/reset": "^0.55.7",
    "@unocss/transformer-directives": "^0.55.7",
    "@unocss/transformer-variant-group": "^0.55.7",
    "@unocss/vite": "^0.55.7",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.33.1",
    "eslint-plugin-vitest": "^0.3.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "svelte-eslint-parser": "^0.33.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  }
}
