{
  "name": "sveltekit-template",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build:site": "vite build",
    "build:lib": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --plugin-search-dir=. --write ."
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.18",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.1",
    "@sveltejs/package": "^2.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@unocss/core": "^0.53.5",
    "@unocss/eslint-config": "^0.53.5",
    "@unocss/extractor-svelte": "^0.53.5",
    "@unocss/preset-attributify": "^0.53.5",
    "@unocss/preset-icons": "^0.53.5",
    "@unocss/preset-uno": "^0.53.5",
    "@unocss/reset": "^0.53.5",
    "@unocss/transformer-directives": "^0.53.5",
    "@unocss/transformer-variant-group": "^0.53.5",
    "@unocss/vite": "^0.53.5",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.32.2",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.5",
    "svelte-eslint-parser": "^0.32.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "vitest": "^0.33.0"
  }
}
