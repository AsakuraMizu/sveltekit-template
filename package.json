{
  "name": "sveltekit-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build:site": "vite build",
    "build:lib": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pret": "prettier --plugin-search-dir=. --check .",
    "pret:fix": "prettier --plugin-search-dir=. --write ."
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.13",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.1",
    "@sveltejs/package": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@unocss/core": "^0.48.0",
    "@unocss/preset-attributify": "^0.48.0",
    "@unocss/preset-icons": "^0.48.0",
    "@unocss/preset-uno": "^0.48.0",
    "@unocss/reset": "^0.48.0",
    "@unocss/transformer-directives": "^0.48.0",
    "@unocss/transformer-variant-group": "^0.48.0",
    "@unocss/vite": "^0.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.14.1",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-eslint-parser": "^0.22.3",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vitest": "^0.26.3"
  }
}
